{
  "name": "spidergraph",
  "version": "0.0.1",
  "description": "GraphQL based web scraper / spider",
  "repository": "https://github.com/skorfmann/spidergraph",
  "author": "Sebastian Korfmann <sebastian@korfmann.net>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "sub": "node -r dotenv/config -r babel-register src/customSubscription.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "jest:integration": "jest test/integration.test.js",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "@types/ioredis": "^3.2.6",
    "@types/node": "^9.4.7",
    "ad-block": "^3.1.11",
    "apollo-server-express": "^1.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "commander": "^2.15.0",
    "cors": "^2.8.4",
    "currency-formatter": "^1.4.2",
    "dataloader": "^1.4.0",
    "es6-template-strings": "^2.0.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "fs-extra": "^5.0.0",
    "geolib": "^2.0.24",
    "graphql": "^0.13.1",
    "graphql-directive": "^0.2.1",
    "graphql-redis-subscriptions": "^1.4.0",
    "graphql-tools": "^2.21.0",
    "ioredis": "^3.2.2",
    "iterall": "^1.2.2",
    "morgan": "^1.9.0",
    "node-geocoder": "^3.22.0",
    "node-telegram-bot-api": "^0.30.0",
    "node-uuid": "^1.4.8",
    "path": "^0.12.7",
    "price-parser": "^2.2.0",
    "pug": "^2.0.1",
    "puppeteer": "^1.1.1",
    "request-promise": "^4.2.2",
    "rimraf": "^2.6.2",
    "rxjs": "6.0.0-alpha.3",
    "subscriptions-transport-ws": "^0.9.6",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.14.0",
    "eslint-watch": "^3.1.3",
    "jest": "^21.2.1",
    "nodemon": "^1.17.1",
    "supertest": "^3.0.0",
    "typescript": "^2.7.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "ignore": [
      "output/*",
      "test/*",
      "logs/*"
    ]
  }
}
