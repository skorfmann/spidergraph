#!/usr/bin/env babel-node --

import loadQueryStore from "../src/queryStore.js";
import program from 'commander';

const scrape = async (url) => {
  const queryStore = await loadQueryStore();
  console.log(JSON.stringify(queryStore))
}

program
  .arguments('<url>')
  .action((url) => (scrape(url)))
  .parse(process.argv);
